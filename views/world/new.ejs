<form action="/" method="POST">
  <input type="text">
</form>

<script>

$(function() {

  var map = {}

  // generate 20 * 20 signed map
  for(var i = -10; i <= 10; i++) {
    map[i] = {}
    for(var j = -10; j <= 10; j++) {
      map[i][j] = 0
    }
  }

  var json = {
    name: "ajax",
    age: 100,
    map: map
  }

  console.log(json)

  var settings = {
    "crossDomain": true,
    "url": "http://localhost:1337/world/",
    "method": "POST",
    "dataType": "json",
    "data": json
  }

  $.ajax(settings).done(function (response) {
    console.log(response);
  });
})

</script>
